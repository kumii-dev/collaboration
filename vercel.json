{
  "buildCommand": "cd apps/web && npm install && npm run build && cd ../api && npm install && npm run build",
  "outputDirectory": "apps/web/dist",
  "installCommand": "echo 'Dependencies installed during build'",
  "framework": null,
  "functions": {
    "api/index.js": {
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/index.js"
    }
  ]
}
