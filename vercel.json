{
  "buildCommand": "npm run vercel-build",
  "outputDirectory": "apps/web/dist",
  "installCommand": "npm install --prefix apps/web && npm install --prefix apps/api",
  "framework": null,
  "functions": {
    "api/index.js": {
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/index.js"
    }
  ]
}
