{
  "buildCommand": "npm --prefix ../../apps/web install && npm --prefix ../../apps/web run build && npm install && npm run build",
  "outputDirectory": "../../apps/web/dist",
  "framework": "vite",
  "installCommand": "echo 'Skipping root install'",
  "functions": {
    "api/index.js": {
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    }
  ]
}
